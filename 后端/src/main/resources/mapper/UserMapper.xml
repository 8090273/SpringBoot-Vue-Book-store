<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.teen.book_store.Mapper.UserMapper">
    <select id="getUserByUsername" parameterType="String" resultType="com.teen.book_store.Bean.User">
        select * from `teen_user` where username = #{username}
    </select>

</mapper>